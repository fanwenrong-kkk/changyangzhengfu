.banner{
	width: 675rpx;
	height: 376rpx;
	border-radius: 10rpx;
	margin: 0 auto;
}
.module{
	width: 675rpx;
	/* height: 200rpx; */
	margin: 20rpx auto 0;
	border-radius: 10rpx;
	background-color: #fff;
	/* padding:40rpx 20rpx; */
}
.pd{
	padding:20rpx;
	width: 635rpx;
}
.module-menu{
	display: -webkit-box;
	display: -webkit-flex;
